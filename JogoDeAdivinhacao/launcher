package projetos.jogoDeAdivinhacao;

import javafx.application.Application;

/**
 * Classe de inicialização da aplicação do Jogo de Adivinhação.
 * Responsável apenas por iniciar o JavaFX.
 */
public class Launcher {

    public static void main(String[] args) {
        Application.launch(JogoDeAdivinhacaoApplication.class, args);
    }
}
