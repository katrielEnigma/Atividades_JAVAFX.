package projetos.montagemDeLanche;

import javafx.application.Application;

/**
 * Classe de inicialização da aplicação de Montagem de Lanche.
 * Responsável apenas por iniciar o JavaFX.
 */
public class Launcher {

    public static void main(String[] args) {
        Application.launch(MontagemDeLancheApplication.class, args);
    }
}
